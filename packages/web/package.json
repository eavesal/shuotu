{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env ENV=local next dev",
    "dev:dev": "cross-env ENV=dev next dev",
    "dev:prod": "cross-env ENV=prod next dev",
    "build": "cross-env-shell ENV=local \"next build && next export\"",
    "build:dev": "cross-env-shell ENV=dev \"next build && next export\"",
    "build:prod": "cross-env-shell ENV=prod \"next build && next export\"",
    "start": "next start",
    "lint": "eslint --ext js,ts,tsx --fix components pages hooks",
    "deploy:dev": "npm run build && node scripts/deploy.js dev",
    "deploy:prod": "npm run build && node scripts/deploy.js prod"
  },
